<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="35dp"
        android:width="35dp">
        <shape android:shape="oval">
            <solid android:color="#fff200" />
        </shape>
    </item>
    <item android:bottom="1dp" android:left="1dp"
          android:right="1dp" android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="#180b0b" />
        </shape>
    </item>
    <item android:height="20dp" android:width="20dp" android:gravity="center" >
        <bitmap  android:src="@drawable/ic_car"
                android:tint="#fcfbfb"/>
    </item>
</layer-list>